<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2.2.dtd">
<web-app>
  <display-name>QFINANCE V1.0 | QMETHODS - Business &amp; IT Consulting GmbH</display-name>
  <description>
     QMETHODS internal finance and project management application.
  </description>

		<context-param>
		  <param-name>PARAMETER_ENCODING</param-name>
		  <param-value>utf-8</param-value>
		</context-param>

    <servlet>
        <servlet-name>QFINANCE</servlet-name>
        <servlet-class>
        	com.qmethods.qfinance.Servlet
        </servlet-class>
        <init-param>
            <param-name>a</param-name>
            <param-value>a</param-value>
        </init-param>        
        <load-on-startup>1</load-on-startup>
    </servlet>    
    <servlet-mapping>
        <servlet-name>QFINANCE</servlet-name>
        <url-pattern>qfinance</url-pattern>
    </servlet-mapping>


</web-app>
